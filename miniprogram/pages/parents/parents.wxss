.head-bg2{
  width: 750rpx;
  height: 1500rpx;
  background-image: url('https://s3.bmp.ovh/imgs/2023/01/13/c38cecc273dc131a.jpg');
  background-size: 100% 100%;
  position:fixed; 
  z-index: -1;
}
.changeInfo{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 90%;
  margin: 50rpx auto;
  padding-top:300px ;
}
.changeInfoName{
  position: relative;
  height: 80rpx;
  width: 100%;
  border-radius: 10rpx;
  background: #fff;
  margin-bottom: 20rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.codeBtn{
  position: absolute;
  right: 0;
  top: 0;
  color: #bbb;
  width: 30%;
  font-size: 26rpx;
  height: 80rpx;
  line-height: 80rpx;
}
.changeInfoName input{
  width: 100%;
  height:100%;
}
.denglu{
  width:250px;
  top:80rpx;
  background-color: rgb(77,132,196);
  color: rgb(253, 253, 253);
  border-radius: 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}



