.head-bg5{
  width: 750rpx;
  height: 1500rpx;
  background-image: url('https://s3.bmp.ovh/imgs/2023/01/13/c38cecc273dc131a.jpg');
  background-size: 100% 100%;
  position:fixed; 
  z-index: -1;
}
/* pages/mine/mine.wxss */
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  background: #7aa3e0;
}
.account-bg {
  width: 100%;
  height: 150rpx;
}
.userinfo-avatar {
  font-size: 50rpx;
  width: 150rpx;
  height: 108rpx;
  margin: 80rpx;
  border-radius: 250%;
}
.userinfo-nickname {
  color: rgb(10, 9, 9);
}
/* 订单 */
.order {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
  height: 90rpx;
}
.myorder-text {
  font-size: 30rpx;
  color: black;
  margin: 20rpx;
  width: 40%;
}
.myorderlook-text {
  font-size: 30rpx;
  color: black;
  position: relative;
  margin: 40rpx;
  width: 60%;
  text-align: left;
}
.next-image {
  width: 20rpx;
  height: 25rpx;
  position: relative;
  right: 10rpx;
}
.navs {
  display: flex;
}
.nav-item {
  width: 25%;
  display: flex;
  align-items: center;
  flex-direction: column;
  padding: 20rpx;
}
.nav-item .nav-image {
  width: 40rpx;
  height: 40rpx;
  margin: 5rpx;
}
.nav-item text {
  margin-top: 20rpx;
  font-size: 25rpx;
  color: gray;
}
/* 列表 */
.person-list {
  display: flex;
  flex-direction: column;
  align-items: left;
}
.list-item {
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 80rpx;
}
.item-image {
  width: 40rpx;
  height: 40rpx;
  margin: 20rpx;
}
.item-text {
  color: black;
  font-size: 30rpx;
  margin-left: -350rpx;
}
.person-line {
  width: 100%;
  height: 2rpx;
  background: lightgray;
  margin-left: 90rpx;
}

